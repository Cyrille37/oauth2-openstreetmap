{
    "name": "jbelien/oauth2-openstreetmap",
    "description": "OpenStreetMap OAuth 2.0 support for the PHP League's OAuth 2.0 Client ",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "openstreetmap",
        "osm"
    ],
    "require": {
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "JBelien\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBelien\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "scripts": {
        "check": "phpcs src test --standard=PSR12 -sp"
    }
}
